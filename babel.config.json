{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "bugfixes": true,
        "debug": false,
        "loose": true
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-proposal-decorators",
      {
        "decoratorsBeforeExport": false,
        "loose": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
